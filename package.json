{
  "name": "@eslint-stylistic/monorepo",
  "type": "module",
  "version": "2.6.3",
  "private": true,
  "packageManager": "pnpm@9.7.1",
  "license": "MIT",
  "scripts": {
    "build": "pnpm -r run build",
    "dev": "pnpm -r --parallel run dev",
    "stub": "pnpm -r --parallel run stub",
    "lint": "eslint . --cache",
    "update": "esno scripts/update.ts && pnpm run lint --fix",
    "publish:ci": "esno scripts/publish.ts",
    "test": "vitest -c vitest.config.unit.ts",
    "test:fixtures": "vitest -c vitest.config.fixtures.ts",
    "check": "pnpm run build && pnpm run update && pnpm run typecheck && pnpm run test --run",
    "typecheck": "vue-tsc --noEmit",
    "prepare": "simple-git-hooks",
    "docs": "pnpm -C docs run docs:dev",
    "docs:build": "pnpm -C docs run docs:build",
    "play:lint": "pnpm -C playground run lint"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.25.1",
    "@antfu/eslint-define-config": "1.23.0-2",
    "@antfu/ni": "^0.22.4",
    "@antfu/utils": "^0.7.10",
    "@babel/eslint-parser": "^7.25.1",
    "@babel/plugin-syntax-decorators": "^7.24.7",
    "@babel/plugin-syntax-do-expressions": "^7.24.7",
    "@babel/plugin-syntax-function-bind": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@eslint-community/eslint-utils": "^4.4.0",
    "@iconify-json/carbon": "^1.1.37",
    "@iconify-json/ri": "^1.1.22",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@shikijs/twoslash": "^1.13.0",
    "@shikijs/vitepress-twoslash": "^1.13.0",
    "@stylistic/eslint-plugin": "workspace:*",
    "@stylistic/eslint-plugin-js": "workspace:*",
    "@stylistic/eslint-plugin-migrate": "workspace:*",
    "@stylistic/eslint-plugin-plus": "workspace:*",
    "@stylistic/eslint-plugin-ts": "workspace:*",
    "@types/eslint": "^9.6.0",
    "@types/estraverse": "^5.1.7",
    "@types/estree": "^1.0.5",
    "@types/fs-extra": "^11.0.4",
    "@types/js-yaml": "^4.0.9",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^22.3.0",
    "@types/picomatch": "^3.0.1",
    "@types/semver": "^7.5.8",
    "@typescript-eslint/parser": "^8.1.0",
    "@typescript-eslint/utils": "^8.1.0",
    "@vitest/coverage-v8": "^2.0.5",
    "@vitest/ui": "^2.0.5",
    "change-case": "^5.4.4",
    "escape-string-regexp": "^5.0.0",
    "eslint": "^9.9.0",
    "eslint-plugin-format": "^0.1.2",
    "eslint-visitor-keys": "^4.0.0",
    "eslint-vitest-rule-tester": "^0.5.2",
    "esno": "^4.7.0",
    "espree": "^10.1.0",
    "estraverse": "^5.3.0",
    "execa": "^9.3.0",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "gray-matter": "^4.0.3",
    "jiti": "^1.21.6",
    "js-yaml": "^4.1.0",
    "json-schema": "^0.4.0",
    "json-schema-to-typescript": "^15.0.0",
    "lint-staged": "^15.2.9",
    "pnpm": "^9.7.1",
    "rimraf": "^6.0.1",
    "rollup": "^4.20.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-esbuild": "^6.1.1",
    "semver": "^7.6.3",
    "shiki": "^1.13.0",
    "simple-git-hooks": "^2.11.1",
    "taze": "^0.16.6",
    "tsx": "^4.17.0",
    "twoslash-eslint": "^0.2.9",
    "typescript": "^5.5.4",
    "unbuild": "^2.0.0",
    "vite": "^5.4.0",
    "vitest": "^2.0.5",
    "vue": "^3.4.37",
    "vue-tsc": "^2.0.29"
  },
  "resolutions": {
    "rollup": "^4.20.0",
    "twoslash-eslint": "^0.2.9",
    "unbuild": "^2.0.0",
    "vite": "^5.4.0",
    "vitest": "^2.0.5",
    "vue": "^3.4.37",
    "vue-tsc": "^2.0.29"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
